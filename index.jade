//output:index.html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(http-equiv="X-UA-Compatible", content="ie=edge")
    link(rel="stylesheet", href="css/style.min.css")
    
    title Document
  body
    .wrapper
      .container
        -var keys=["A","S","D","F","G","H","J","K","L",]
        -var instrument=["clap","hithat","kick","openhat","boom","ride","snare","tom","tink"]
        -var keycodes=[65,83,68,70,71,72,74,75,76]
        -for(i=1;i<=9;i++)
            div(class="item"+ " item"+i id="kk-"+keycodes[i-1])
            
              h1=keys[i-1]
              p=instrument[i-1].toUpperCase()
              audio(id="ss-"+keycodes[i-1])
                source(src="sounds/"+instrument[i-1]+".wav")
      
          //- audio(controls="controls")
          //-   source(src="sounds/clap.wav" type="audio/wav")
    script(src="js/script.js")
      
            
            
        
      
      
